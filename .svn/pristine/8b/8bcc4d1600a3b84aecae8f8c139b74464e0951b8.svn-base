<html><head>
  <SCRIPT src="./web-files/dtjava.js"></SCRIPT>
<script>
    function launchApplication(jnlpfile) {
        dtjava.launch(            {
                url : 'ERP.jnlp',
                jnlp_content : ''
            },
            {
                javafx : '8.0+'
            },
            {}
        );
        return false;
    }
</script>

<script>
    function javafxEmbedcom.j3technology.application() {
        dtjava.embed(
            {
                id : 'com.j3technology.application',
                url : 'ERP.jnlp',
                placeholder : 'javafx-app-placeholder',
                width : '800',
                height : '600',
                jnlp_content : ''
            },
            {
                javafx : '8.0+'
            },
            {}
        );
    }
    <!-- Embed FX application into web page once page is loaded -->
    dtjava.addOnloadCallback(javafxEmbedcom.j3technology.application);
</script>

</head><body>
<h2>Test page for <b>ERP</b></h2>
  <b>Webstart:</b> <a href='ERP.jnlp' onclick="return launchApplication('ERP.jnlp');">click to launch this app as webstart</a><br><hr><br>

  <!-- Applet will be inserted here -->
  <div id='javafx-app-placeholder'></div>
</body></html>
